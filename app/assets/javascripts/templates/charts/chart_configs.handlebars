	<li class='filterDiv'>
		<a rel="popover" class="data_source_popover" data-toggle="tooltip" data-placement="right auto" title="Edit Chart Configs" {{bindAttr id='dataSourcePopoverId'}}  >
			<i class='icon-cog muted'></i>
		</a>

		<div class="popover fade bottom in hidden" style="display: block; height: auto; ">
			<div class="arrow hidden" style="left:64px;"></div>
			<h3 class="popover-title hidden">Chart Configs</h3>
			<div class="popover-content hidden">
				<form class='form form-inline' >
					<div >

						{{#if view.bar}}
						  {{partial 'charts/edit_configs/bar'}}
						{{/if}}

						{{#if view.ctable}}
							{{partial 'charts/edit_configs/ctable'}}
						{{/if}}

						{{#if view.geo}}
							{{partial 'charts/edit_configs/geo'}}
						{{/if}}

						{{#if view.heatmap}}
							{{partial 'charts/edit_configs/heatmap'}}
						{{/if}}

						{{#if view.line}}
							{{partial 'charts/edit_configs/line'}}
						{{/if}}

						{{#if view.table}}
							{{partial 'charts/edit_configs/table'}}
						{{/if}}

						{{#if view.multiline}}
							{{partial 'charts/edit_configs/multiline'}}
						{{/if}}

						{{#if view.single_value}}
							{{partial 'charts/edit_configs/singleValue'}}
						{{/if}}	

						{{#if view.grouped_table}}
							{{partial 'charts/edit_configs/groupedTable'}}
						{{/if}}				

					</div>
					{{partial 'charts/edit_configs/common'}}
					<div class="row">
						<div class="span2" style="margin-top:10px;">
							<button type="submit" class='btn' >Save</button>
							<button class="btn" aria-hidden="true" onclick="$(this).closest('li').find('.icon-cog').click();return false;">Cancel</button>
						</div>
					</div>
				</form>
				</div>
			</div>
	</li>
